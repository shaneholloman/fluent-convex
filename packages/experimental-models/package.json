{
  "name": "@fluent-convex/experimental-models",
  "version": "0.1.0",
  "description": "Experimental model functionality for fluent-convex",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --typecheck",
    "dev:test": "vitest --typecheck",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "convex",
    "fluent-convex",
    "models",
    "experimental"
  ],
  "author": "Mike Cann",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "convex": "^1.28.0",
    "fluent-convex": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@vitest/ui": "^2.1.8",
    "convex": "^1.28.0",
    "fluent-convex": "file:../fluent-convex",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "vitest": "^2.1.8"
  }
}
